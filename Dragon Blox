local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("Dragon Blox Ultimate GUI", 5013109572)

-- themes
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}


local page = venyx:addPage("AutoFarm", 5012544693)
local section1 = page:addSection("Farm")
local ss = false
section1:addToggle("Auto Strength and Speed", nil, function()
    if ss == false then
        ss = true
        repeat 
            wait()
            local A_1 = "Blacknwhite27"
            local A_2 = 1
            local Event = game:GetService("ReplicatedStorage").Package.Events.p
            Event:FireServer(A_1, A_2)
        until ss == false
    else
        ss = false
end
end)
local defence = false
section1:addToggle("Auto Defence", nil, function()
    if defence == false then
        defence = true
        repeat
            wait()
            local A_1 = "Blacknwhite27"
            local Event = game:GetService("ReplicatedStorage").Package.Events.def
            Event:InvokeServer(A_1)
        until defence == false
    else
        defence = false
end
end)
local  energy = false
section1:addToggle("Auto energy", nil, function()
    if energy == false then
        energy = true
        repeat
            wait()
            local A_1 = 2
            local A_2 = true
            local Event = game:GetService("ReplicatedStorage").Package.Events.kb
            Event:FireServer(A_1, A_2)
        until energy == false
    else
        energy = false
end
end)
local  charge = false
section1:addToggle("Auto Charge", nil, function()
    if charge == false then
        charge = true
        repeat
            wait()
            local A_1 = "Blacknwhite27"
            local Event = game:GetService("ReplicatedStorage").Package.Events.cha
            Event:InvokeServer(A_1)
        until charge == false
    else
        charge = false
end
end)
local  fighter = false
section1:addToggle("Auto Fighter X", nil, function()
    if fighter == false then
        fighter = true
        repeat
            wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Living:WaitForChild("X Fighter").HumanoidRootPart.CFrame
            local A_1 = "Blacknwhite27"
            local A_2 = 1
            local Event = game:GetService("ReplicatedStorage").Package.Events.p
            Event:FireServer(A_1, A_2)
        until fighter == false
    else
        fighter = false
end
end)
local page = venyx:addPage("Quests", 5012544693)
local section1 = page:addSection("---this toggles only accept challenges---")
local section1 = page:addSection("Quests:")
local  Xfighter = false
section1:addToggle("Auto X Fighter Trainer", nil, function()
    if Xfighter == false then
        Xfighter = true
        repeat
            wait()
            local A_1 = game:GetService("Workspace").Others.NPCs["X Fighter Trainer"]
            local Event = game:GetService("ReplicatedStorage").Package.Events.Qaction
            Event:InvokeServer(A_1)
            wait(40)
        until Xfighter == false
    else
        Xfighter = false
end
end)


